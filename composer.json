{
  "name": "siberfx/lara-meta",
  "keywords": ["meta tags", "meta", "tags", "head", "laravel"],
  "description": "A package to manage Header Meta Tags for Laravel 6.0",
  "version": "1.0",
  "license": "MIT",
    "authors": [
        {
            "name": "Selim GORMUS",
            "email": "info@siberfx.com",
            "homepage": "https://siberfx.com"
        }
    ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Siberfx\\LaraMeta\\": "src/Siberfx/LaraMeta/"
    }
  },
  "extra": {
        "laravel": {
            "providers": [
                "Siberfx\\LaraMeta\\MetaTagServiceProvider"
            ],
            "aliases": {
                "Image": "Siberfx\\LaraMeta\\MetaTag\\Facades\\MetaTag"
            }
        }
    },
    "minimum-stability": "stable"
}
