{
  "name": "siberfx/lara-meta",
  "description": "A package to manage Header Meta Tags for Laravel 7-8",
  "keywords": [
    "share",
    "laravel",
    "laravel7",
    "laravel8"
  ],
  "version": "2.2.1",
  "license": "MIT",
  "authors": [
    {
      "name": "siberfx",
      "email": "info@siberfx.com"
    }
  ],
  "require": {
    "php": "^7|^8",
    "illuminate/support": "^7|^8"
  },
  "autoload": {
    "psr-0": {
      "Siberfx\\LaraMeta\\": "src/"
    }
  },
    "extra": {
    "laravel": {
      "providers": [
        "Siberfx\\LaraMeta\\MetaTagsServiceProvider"
      ],
      "aliases": {
        "MetaTag": "Siberfx\\LaraMeta\\Facades\\MetaTag"
      }
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^1.3"
  }
}
