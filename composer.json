{
  "name": "siberfx/lara-meta",
  "keywords": ["meta tags", "meta", "tags", "head", "laravel"],
  "description": "A package to manage Header Meta Tags for Laravel 6.0",
  "license": "MIT",
    "authors": [
        {
            "name": "Selim GORMUS",
            "email": "info@siberfx.com",
            "homepage": "https://siberfx.com"
        }
    ],
  "minimum-stability": "stable",
  "require": {
    "php": "8|8.1|9"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "extra": {
    "laravel": {
        "providers": [
            "Siberfx\\LaraMeta\\MetaTagsServiceProvider"
        ],
        "aliases": {
            "MetaTag": "Siberfx\\LaraMeta\\Facades\\MetaTag"
        }
    }
},
  "autoload": {
    "psr-4": {
      "Siberfx\\LaraMeta\\": "src/Siberfx/LaraMeta/"
    }
  },
  "autoload-dev": {
        "psr-4": {
          
        }
    }
}
