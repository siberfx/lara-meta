{
  "name": "siberfx/lara-meta",
  "keywords": [
    "meta tags",
    "meta",
    "tags",
    "head",
    "laravel"
  ],
  "description": "A package to manage Header Meta Tags for Laravel 11+",
  "license": "MIT",
  "authors": [
    {
      "name": "Selim GORMUS",
      "email": "info@siberfx.com",
      "homepage": "https://siberfx.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.2|^8.3|^8.4",
    "illuminate/support": "^11|^12",
    "illuminate/http": "^11|^12",
    "laravel/helpers": "^1.6"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Siberfx\\LaraMeta\\MetaTagsServiceProvider"
      ],
      "aliases": {
        "MetaTag": "Siberfx\\LaraMeta\\Facades\\MetaTag"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Siberfx\\LaraMeta\\": "src/Siberfx/LaraMeta/"
    }
  }
}
