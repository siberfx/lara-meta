{
  "name": "siberfx/lara-meta",
  "keywords": ["meta tags", "meta", "tags", "head", "laravel"],
  "description": "A package to manage Header Meta Tags for Laravel 8+",
  "license": "MIT",
    "authors": [
        {
            "name": "Selim GORMUS",
            "email": "info@siberfx.com",
            "homepage": "https://siberfx.com"
        }
    ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2",
    "illuminate/support": "^7|^8|^9|^10"
  },
  "require-dev": {
     "phpunit/phpunit": "^8.0 || ^9.0 || ^10.0",
     "scrutinizer/ocular": "~1.7"
  },
  "extra": {
    "laravel": {
        "providers": [
            "Siberfx\\LaraMeta\\MetaTagsServiceProvider"
        ],
        "aliases": {
            "MetaTag": "Siberfx\\LaraMeta\\Facades\\MetaTag"
        }
    }
},
  "autoload": {
    "psr-4": {
      "Siberfx\\LaraMeta\\": "src/Siberfx/LaraMeta/"
    }
  }
}
